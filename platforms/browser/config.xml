<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.IMS" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>IMS</name>
    <description>
        Inventry Management Sytem
    </description>
    <author email="abid_akhund13@yahoo.com" href="http://phonegap.com">
        PhoneGap Team
    </author>
    <content src="patient_form.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="16" />
</widget>
